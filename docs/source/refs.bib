  shorttitle = {Green composites},
  url        = {https://onlinelibrary.wiley.com/doi/abs/10.1002/pc.21224},
  urldate    = {2020-07-29},
}

@Article{Ku2011,
  author   = {Ku, H. and Wang, H. and Pattarachaiyakoop, N. and Trada, M.},
  journal  = {Composites Part B: Engineering},
  title    = {A review on the tensile properties of natural fiber reinforced polymer composites},
  year     = {2011},
  issn     = {1359-8368},
  month    = jun,
  number   = {4},
  pages    = {856--873},
  volume   = {42},
  abstract = {This paper is a review on the tensile properties of natural fiber reinforced polymer composites. Natural fibers have recently become attractive to researchers, engineers and scientists as an alternative reinforcement for fiber reinforced polymer (FRP) composites. Due to their low cost, fairly good mechanical properties, high specific strength, non-abrasive, eco-friendly and bio-degradability characteristics, they are exploited as a replacement for the conventional fiber, such as glass, aramid and carbon. The tensile properties of natural fiber reinforce polymers (both thermoplastics and thermosets) are mainly influenced by the interfacial adhesion between the matrix and the fibers. Several chemical modifications are employed to improve the interfacial matrix–fiber bonding resulting in the enhancement of tensile properties of the composites. In general, the tensile strengths of the natural fiber reinforced polymer composites increase with fiber content, up to a maximum or optimum value, the value will then drop. However, the Young’s modulus of the natural fiber reinforced polymer composites increase with increasing fiber loading. Khoathane et al. [1] found that the tensile strength and Young’s modulus of composites reinforced with bleached hemp fibers increased incredibly with increasing fiber loading. Mathematical modelling was also mentioned. It was discovered that the rule of mixture (ROM) predicted and experimental tensile strength of different natural fibers reinforced HDPE composites were very close to each other. Halpin–Tsai equation was found to be the most effective equation in predicting the Young’s modulus of composites containing different types of natural fibers.},
  doi      = {10.1016/j.compositesb.2011.01.010},
  file     = {ScienceDirect Full Text PDF:https\://pdf.sciencedirectassets.com/271637/1-s2.0-S1359836811X00045/1-s2.0-S1359836811000382/main.pdf?X-Amz-Security-Token=IQoJb3JpZ2luX2VjEE4aCXVzLWVhc3QtMSJHMEUCIBUyh%2F1VgV11BYr2cHPXFcsEiq1kG0riiZ%2BuCaJQsIHWAiEA6w6ePb4PfOQqlDwj55qGiy4X8S5UixJCzzJb%2Bw%2BER9UqtAMIFxADGgwwNTkwMDM1NDY4NjUiDLKg%2BhZYmL06RAhGsCqRA0h2EPOGNdKeeyvviZm83pxhocve5tzL1F8WcNjgAK1UAojs2UGYhTe2Fc8n9YI%2BHJW%2FJIlmjbHkr95xZndi1Ecj85FrsqvtMvHWUW1LbZWs1%2Br5VPOBt5OHTR1zb%2FlrB3jRZAHLF1OlS3HHWjIujTiK2hsV%2Bo66pgOYFLGAwClmYySbPbSY6WeM8kYmsEXbYAEIl93gfYyEilUnwMs10eCRi7jHRvTMcFOUI8Qz7K7FQjnlXPgY2ENCEDn1KCAQbvo0p94FK3Y7%2Bd2ErQM36LpkLibpZ504cwSWh4VQaSd1BsQTHATJnsuiI43C4JXMEEkDPL9j2sSyXr9mke1gVk99JgpBV%2BvZl8MJlqigp2LX1deUCiTXoe%2BmD377hb4HFf6Lclt96GLjm6eZ0IZnaM%2BiJWouFJz68nAwkqBI0PUgD0JSwcrt7SUcYB8bXwD8M7nw9%2FNFgsM4hrdcJ93dqqtrsJ147CXqb9H%2Fx%2F2IsgyMVIHaRH3eEcR%2FadUvXJezI8ts4eEsLcHGdNkAjGz1RcCPMLf1hfkFOusBVECEGhK0TzZO8afjwKPSkSt40WVGSA8HOYUv6j%2FAmDGc1NoD30A3bRbW4UNZ%2FnDbbooG9LHfm5A%2Bb6%2FwOL66zctvAS%2FcHJK5SiDap%2FnIfdvgE38SCLQnYDBUJIDQrlppX%2B73SWnPiexJ6hu%2Bw1J2x7twtoM%2BP1oa0etrbQRd9ZLU0fitK5medCOvwdSzCu1AR7GORKTe9Z8gXbqdIsN%2BDTEa09ZSmPTJBeSGQKgbZpHZ2MDohn%2BmJp5mSznagnZbJD9bHXMA%2FCijy0AumMLcOAXgLFFnN8L3U4aisZwrcIHFwqVAMC2bPhzj9A%3D%3D&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Date=20200729T140210Z&X-Amz-SignedHeaders=host&X-Amz-Expires=300&X-Amz-Credential=ASIAQ3PHCVTY65XCXSVA%2F20200729%2Fus-east-1%2Fs3%2Faws4_request&X-Amz-Signature=4e43861f309654be16b12c5f5fefd18c49e6f57b993a2d18ac0ea0dbaae6911b&hash=c6c5b6cc1932198d0435f39f293c5058be6b58b9c9dff15237c0c6a36ac1bf89&host=68042c943591013ac2b2430a89b270f6af2c76d8dfd086a07176afe7c76c2c61&pii=S1359836811000382&tid=spdf-59998422-6d9c-4037-8d5c-44567d93a946&sid=62ab6d689217c940e32a26f2c237a3401211gxrqb&type=client:application/pdf},
  keywords = {A. Polymer-matrix composites (PMCs), B. Mechanical properties, D. Mechanical testing, E. Compression moulding},
