@Article{Vallo2004,
  author    = {Vallo, Claudia and Kenny, Jose M. and Vazquez, Analia and Cyras, Viviana P.},
  journal   = {Journal of Composite Materials},
  title     = {Effect of {Chemical} {Treatment} on the {Mechanical} {Properties} of {Starch}-{Based} {Blends} {Reinforced} with {Sisal} {Fibre}},
  year      = {2004},
  issn      = {0021-9983},
  month     = aug,
  number    = {16},
  pages     = {1387--1399},
  volume    = {38},
  abstract  = {Biocomposites were produced using polycaprolactone (PCL) and starch as the matrix, and washed and treated sisal fibres as reinforcement. The matrix consists of a biodegradable product commercially known as MaterBi-Z®, which is based on a PCL, starch and additive., Alkaline and acetylation treatments were performed on the fibre in order to enhance the adhesion degree and the compatibility between the fibre and the matrix. The effect of chemical treatment on morphology, physical and chemical properties and tensile properties of fibres and composites were determined. Tensile properties of the biodegradable composite were improved by the presence of the fibre. However, the untreated fibres behaved as better reinforcement than the acetylated and alkalitreated fibres. This was attributed to an impairment of the mechanical properties of the acetylated fibres and incompatibility of the alkali-treated fibres. The results observed are supported by SEM analysis of fibres and the composite materials.},
  doi       = {10.1177/0021998304042738},
  file      = {SAGE PDF Full Text:https\://journals.sagepub.com/doi/pdf/10.1177/0021998304042738:application/pdf},
  language  = {en},
  publisher = {SAGE Publications Ltd STM},
  url       = {https://doi.org/10.1177/0021998304042738},
  urldate   = {2020-07-29},
}

@Article{Mishra2003,
  author   = {Mishra, S and Mohanty, A. K and Drzal, L. T and Misra, M and Parija, S and Nayak, S. K and Tripathy, S. S},
  journal  = {Composites Science and Technology},
  title    = {Studies on mechanical performance of biofibre/glass reinforced polyester hybrid composites},
  year     = {2003},
  issn     = {0266-3538},
  month    = aug,
  number   = {10},
  pages    = {1377--1385},
  volume   = {63},
  abstract = {The degree of mechanical reinforcement that could be obtained by the introduction of glass fibres in biofibre (pineapple leaf fibre/sisal fibre) reinforced polyester composites has been assessed experimentally. Addition of relatively small amount of glass fibre to the pineapple leaf fibre and sisal fibre-reinforced polyester matrix enhanced the mechanical properties of the resulting hybrid composites. Different chemically modified sisal fibres have been used in addition to glass fibers as reinforcements in polyester matrix to enhance the mechanical properties of the resulting hybrid composites. The surface modification of sisal fibres such as alkali treatment produced optimum tensile and impact strengths, while cyanoethylation resulted in the maximum increase in flexural strength of the hybrid composites. It has been observed that water uptakes of hybrid composites are less than that of unhybridized composites. Scanning electron microscopic studies have been carried out to study the fibre-matrix adhesion.},
  doi      = {10.1016/S0266-3538(03)00084-8},
  file     = {ScienceDirect Full Text PDF:https\://pdf.sciencedirectassets.com/271518/1-s2.0-S0266353800X02321/1-s2.0-S0266353803000848/main.pdf?X-Amz-Security-Token=IQoJb3JpZ2luX2VjEEoaCXVzLWVhc3QtMSJHMEUCIEK4VdxoP104qpxGo2RHJC62L8VuUVEcl6lRDkyfKW12AiEApmgo1BmYX4FiUYrfE5mkJ0%2BGBdsk4FAx6WF495xs2MQqtAMIExADGgwwNTkwMDM1NDY4NjUiDNGCuiZ%2BbXTZH5uArSqRAxnw1zkGItVIdsfhIwrkXDwejkIUnO0sCs1p%2BOxkf0knCxFcnI%2FueXkE2YGuFCUYSHNkdZmpGdCgO3dO8vbXB4R4HpFwmj1kT4mFtqIMjDlc3busoM%2B0rd3uC5OyafMJPqYpvBsH2Y3YfTmeZ5YnybcWHXb4Ze8PiI7fBElBxGG2UdHCOqPf%2FnIUURiwdiFch9UvuvhO3DFQDU8GhoXS2Z2umyRz9dazXN2IW%2B%2F6ejz7S3Wop2cuJK%2FHsieBn0vKstZDE7yMU4C4uUcbYod%2B7TxMrVDk0qe6yBZGkKoo%2FwwFS6sWMr7CTTG%2Fc5OiK8nHyN9hTJBAjS%2B8zAlEFerBdsxjsp%2BLkL6Ia%2Fl8N00OfN4zw38zDNfXrIKWHdTk%2BMAOlQZDle3qz%2B49B8uqE1R4tRJgzv7DRe6Vl5YiVrQfnxywHsKIiZXgxFKKYfQ7NIleomAPSOsATBqDeskem2MFdxycPEq6GKEzNts5O4bdYZDIZ3F9EGs5L5wm8weHzKAisFadNTowsk75Rz0dvn%2F3SxCGMJmFhfkFOusBXQZDudRZgJkOwlCaM2kzjShYAkBQJiqL5Ia3l4mqqgMC%2Fs5e%2FeFngqoOOk3TAjTqV%2F3QjW2QPi9WNe9ZUrxL1cwDDNm0UwvB8VmSUS8ZKCZHYIWRXyaiojhkSNRhaEuu8ceI4KwWTxXnkegRy4cBzDBKsTHKZS%2B%2F1Wp4ISbDchzzIqTIz2Ov2wnSFpolMrj2eVoSC65CGN77AA4%2BVIceuBSr6eqn2W9gsX93tzowh7ruefqV32Nu9PTz4yTAfJtLn2JF1mgEpofTQziG3oX6LjRIgCPotuH%2FIiS5lG2vbp7HeeTZydLLTZJdmA%3D%3D&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Date=20200729T101248Z&X-Amz-SignedHeaders=host&X-Amz-Expires=300&X-Amz-Credential=ASIAQ3PHCVTYXH65GZ4N%2F20200729%2Fus-east-1%2Fs3%2Faws4_request&X-Amz-Signature=9db178ad7eab6e6636f8a9fbf376f8aaf11f35cca82e3720477fa2ed89d522e1&hash=e86a91f7df4c5dc5cd7265867f601292a5f767d628f8918d9975f4f9655390af&host=68042c943591013ac2b2430a89b270f6af2c76d8dfd086a07176afe7c76c2c61&pii=S0266353803000848&tid=spdf-6e5db151-4fbf-4776-a318-37993d782346&sid=6c57fa2c5df636494d5b2d63644c8f0fb75dgxrqb&type=client:application/pdf},
