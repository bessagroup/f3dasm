@Article{Wang2007,
  author    = {Wang, B. and Panigrahi, S. and Tabil, L. and Crerar, W.},
  journal   = {Journal of Reinforced Plastics and Composites},
  title     = {Pre-treatment of {Flax} {Fibers} for use in {Rotationally} {Molded} {Biocomposites}},
  year      = {2007},
  issn      = {0731-6844},
  month     = mar,
  number    = {5},
  pages     = {447--463},
  volume    = {26},
  abstract  = {The objective of this study was to determine the effects of pre-treated flax fibers on the performance of the fiber-reinforced composites. Lack of good interfacial adhesion and poor resistance to moisture absorption make the use of natural fiber-reinforced composites less attractive. In order to improve fiber/matrix interfacial properties, fibers were subjected to chemical treatments, namely, mercerization, silane treatment, benzoylation, and peroxide treatment. Selective removal of non-cellulosic compounds constitutes the main objective of the chemical treatments of flax fibers to improve the performance of fiber-reinforced composites. Flax fibers were derived from Saskatchewan-grown flax straws. Composites consisting of high-density polyethylene (HDPE) or linear low-density polyethylene (LLDPE) or HDPE/LLDPE mix, chemically treated fibers and additives were prepared by the extrusion process. The test samples were prepared by rotational molding. The fiber surface morphology and the tensile fracture surfaces of the composites were characterized by scanning electron microscopy (SEM). The effects of the different chemical treatments on the mechanical and the physical properties of natural fiber-reinforced composites were investigated. The differential scanning calorimetry (DSC) was used to measure the melting point of the fiber-reinforced composites.},
  doi       = {10.1177/0731684406072526},
  file      = {SAGE PDF Full Text:https\://journals.sagepub.com/doi/pdf/10.1177/0731684406072526:application/pdf},
  language  = {en},
  publisher = {SAGE Publications Ltd STM},
  url       = {https://doi.org/10.1177/0731684406072526},
  urldate   = {2020-07-29},
}

@Article{Seki2009,
  author   = {Seki, Yoldas},
  journal  = {Materials Science and Engineering: A},
  title    = {Innovative multifunctional siloxane treatment of jute fiber surface and its effect on the mechanical properties of jute/thermoset composites},
  year     = {2009},
  issn     = {0921-5093},
  month    = may,
  number   = {1},
  pages    = {247--252},
  volume   = {508},
  abstract = {Natural fiber reinforced polymer composites have many applications because of their ease of fabrication, relatively low cost, low density and renewable resource. In spite of the various desirable properties of natural fiber to act as a reinforcing material, poor adhesion characteristics between natural fiber and polymer resin result in low mechanical properties. In this study, jute–thermoset composites were fabricated by using unsaturated polyester and epoxy resins. To improve the adhesion between jute fabric and thermoset, alkali treated jute fibers were treated with oligomeric siloxane. FTIR analysis was used to confirm the surface treatment. The effects of fiber surface treatment on the mechanical properties of jute reinforced thermoset composites were determined by using tensile test, flexure test and short beam shear test. The fractured surfaces of composites were investigated by scanning electron microscopic (SEM) technique. Once jute fabrics were treated 1\% siloxane concentration, the tensile and flexure properties of silane treated jute thermoset composites increased. Surface treatment of jute fiber caused a significant increase in the interlaminar shear strength (ILSS) of the thermoset composites. From SEM observations, better adhesion was observed for the jute/thermoset composites in the presence of oligomeric siloxane.},
  doi      = {10.1016/j.msea.2009.01.043},
  file     = {ScienceDirect Full Text PDF:https\://pdf.sciencedirectassets.com/271387/1-s2.0-S0921509309X00080/1-s2.0-S0921509309000690/main.pdf?X-Amz-Security-Token=IQoJb3JpZ2luX2VjEEoaCXVzLWVhc3QtMSJIMEYCIQCD31vs3z4J6I6FOUf73s5C7qeQn9w8JlsmwlVqGigiNAIhAPcCd7KL8Qn8WL19nJCtQwhRilgnRacLSsGAVBl4JU1pKrQDCBMQAxoMMDU5MDAzNTQ2ODY1Igy9jFrhRZlt3rMpbJwqkQORkefbKqmY0Z1Bu2pIo9q59lDNaPTmIlTYP3iqS%2BwAEI5X7VxqZDJQL3RWB2KkmzHMHy9EAIXwlIjZ%2FB6hW4RIyjZnCBj3yeLb7DoBK4bOF%2Fvc85t5HR%2FvtykvNxplM78cgb5RpAPo%2F6un%2Byz%2F735opeVzNmNglmUedOAGD68fhkpCgJLKAmXe1wTgJbc5QahM%2BHMkXk%2FozeIbe3A5kVGok5G6zlX6Cf5UmQiWGuQnCwueiKIjqf6ahr%2BSfi8aea1sM%2Bv298IHchCTWB1Jj7TYw2pe%2FxvtH2c3X9L0Ry%2BSBNnCgNMNf0anu5sFIYCz1NQy6xp3HcS7Ed1dxvKAICAs8BERHADq%2F1uZzy%2FgJR3yno67BjUZyXbP4q0QUlKwRmn6IEUez0koFE6vP7TNBvSjCbWsir2zHYTMGidwzLRlCvO2fC7qHw%2FZ98Q9Ys5Rri1wP7f8OVvOKnWFgTK%2B4bbgbkM8e96JG3Obbbpkv6p5NXYTIpYDKgcw%2FpwcK6%2BKdVKJ1CVyP%2ByGJaMuQHBAu%2FlazzDHgYX5BTrqAcJ1F9PNP0WLuxlfRXcUssm4lp8aMKw%2Fk9cp4zrCz7e0IFB5YofMNbEfAWaTO7Ac01LpgavRNKWzSUik9DR41nXDsbDRnCiTAxMV67CzXkZ0VkTwbjWVgSBqb%2B6M%2B%2Bhuh6QUVArhNZZT0X4F%2FP99nj4HtZ0UUOCcHrlCX6k0%2BuFTOey5o86eJyyHZSJyO92S0ma3CbSarXHy6ffaDpGJZBRv8dv%2BXdqkB7F%2B61IkluxwObYnkRvr5rbS69HLtDp8jQU0i%2BasPp0ul7MXthpcPwv3lIsHtIyVrsVD%2BtCqruGsBYDXdV%2B%2FTZBjqw%3D%3D&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Date=20200729T102415Z&X-Amz-SignedHeaders=host&X-Amz-Expires=300&X-Amz-Credential=ASIAQ3PHCVTY5HAI6LH2%2F20200729%2Fus-east-1%2Fs3%2Faws4_request&X-Amz-Signature=c9cdaa791216f12dd3af071a2d5119b3f29443b4ae18bf54b91e615c490bb4bf&hash=777756d116109a39842ec33083a0c8cdaed78cd428e5813c05533fe8682501c1&host=68042c943591013ac2b2430a89b270f6af2c76d8dfd086a07176afe7c76c2c61&pii=S0921509309000690&tid=spdf-fc040f27-8999-40f7-a06d-103a9a3d5609&sid=6c57fa2c5df636494d5b2d63644c8f0fb75dgxrqb&type=client:application/pdf},
