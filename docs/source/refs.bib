@InProceedings{Shi2020,
  author    = {Shi, Lei and Zhang, Yifan and Wang, Wanguo and Cheng, Jian and Lu, Hanqing},
  booktitle = {2020 {IEEE} {International} {Conference} on {Multimedia} and {Expo} ({ICME})},
  title     = {Rethinking {The} {Pid} {Optimizer} {For} {Stochastic} {Optimization} {Of} {Deep} {Networks}},
  year      = {2020},
  month     = jul,
  note      = {ISSN: 1945-788X},
  pages     = {1--6},
  abstract  = {Stochastic gradient descent with momentum (SGD-Momentum) always causes the overshoot problem due to the integral action of the momentum term. Recently, an ID optimizer is proposed to solve the overshoot problem with the help of derivative information. However, the derivative term suffers from the interference of the high-frequency noise, especially for the stochastic gradient descent method that uses minibatch data in each update step. In this work, we propose a complete PID optimizer, which weakens the effect of the D term and adds a P term to more stably alleviate the overshoot problem. To further reduce the interference of the high-frequency noise, two effective and efficient methods are proposed to stabilize the training process. Extensive experiments on three widely used benchmark datasets with different scales, i.e., MNIST, Cifar10 and TinyImageNet, demonstrate the superiority of our proposed PID optimizer on various popular deep neural networks.},
  doi       = {10.1109/ICME46284.2020.9102970},
