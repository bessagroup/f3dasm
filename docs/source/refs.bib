@Article{Mwaikambo2007,
  author    = {Mwaikambo, Leonard Y. and Tucker, Nick and Clark, Andrew J.},
  journal   = {Macromolecular Materials and Engineering},
  title     = {Mechanical {Properties} of {Hemp}-{Fibre}-{Reinforced} {Euphorbia} {Composites}},
  year      = {2007},
