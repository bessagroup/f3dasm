@InCollection{Boufi2017,
  author     = {Boufi, Sami},
  booktitle  = {Lignocellulosic Fibre and Biomass-Based Composite Materials},
  publisher  = {Woodhead Publishing},
  title      = {18 - {Biocomposites} from olive-stone flour: {A} step forward in the valorization of the solid waste from the olive-oil industry},
  year       = {2017},
  editor     = {Jawaid, Mohammad and Md Tahir, Paridah and Saba, Naheed},
  isbn       = {9780081009598},
  month      = jan,
  pages      = {387--408},
  series     = {Woodhead {Publishing} {Series} in {Composites} {Science} and {Engineering}},
  abstract   = {Spain, Greece, Italy, Tunisia, and Turkey are the most important olive-oil producers in the Mediterranean basin representing about 95\% of the world production. The undeniable nutritional merits of olive-oil are well known, thus increasing the demand for it. However, if the undeniable nutritional merits of olive-oil are well known, the extraction process generates an amount of solid residue called “olive cake,” which is obtained after extracting the oil from the fruit. Currently, from 100kg of olives, about 20\% of oil is recovered, and 30\% of olive cake and about 50\% of aqueous liquor are generated. The use of the ground solid waste as a sustainable filler in polymer-based composite might constitute an interesting valorization within the field of material technology. In fact, given the high hardness of the olive stones, an enhancement in the stiffness of the polymer when the ground olive-stone residue is incorporated within a thermoplastic polymer matrix is achieved. This chapter summarizes the most recent advances of the use of olive solid residue ground in the form of flour (OSF) as a lignocellulosic filler for wide ranges of thermoplastic matrix, including polypropylene, polyvinylchloride, and polyethylene, and analyzes the impact of the effect of the incorporation of the OSF on the tensile properties, flexural properties, impact strength, thermal properties, and abrasion resistance.},
