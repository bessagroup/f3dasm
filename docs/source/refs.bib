@Article{Kerni2020,
  author   = {Kerni, Love and Singh, Sarbjeet and Patnaik, Amar and Kumar, Narinder},
  journal  = {Materials Today: Proceedings},
  title    = {A review on natural fiber reinforced composites},
  year     = {2020},
  issn     = {2214-7853},
  month    = jan,
  pages    = {1616--1621},
  volume   = {28},
  abstract = {Materials plays important role for survival of any manufacturing industry. Composite materials replace the conventional materials because of their high specific strength, strong damping capacity and high specific modulus. In modern era, natural fiber reinforced polymer composites came into light because of promising properties of natural fibers such as light weight, water resistance, high impact strength, environment friendly etc. In this study, different types of natural fibers that can be used as reinforcement in polymer composite are discussed. Various methods of production and steps involved in processing of natural fiber reinforced composite are presented. Then, mechanical and tribological properties of these composites are reviewed and presented. The different applications of natural fiber reinforced polymer composite are also discussed.},
  doi      = {10.1016/j.matpr.2020.04.851},
  file     = {ScienceDirect Full Text PDF:https\://pdf.sciencedirectassets.com/305927/1-s2.0-S2214785320X0037X/1-s2.0-S2214785320335057/main.pdf?X-Amz-Security-Token=IQoJb3JpZ2luX2VjEEgaCXVzLWVhc3QtMSJHMEUCIQCBZzag5KjDSzMBjBheoli42Li5DXK0LDEkHr0OFpbO0wIgYpzNU%2BW7zoeO%2FdVDjHEr1rZSyuNe4en61aG2O%2BAdURQqtAMIEBADGgwwNTkwMDM1NDY4NjUiDEy7P3w09uEmnC6xRCqRA12Z8DPpMJ1z5lQ0xbH%2FYmBxOOiB1IGp4rw3PTEYTqsuSf9RPBYcwMD9nG3uQoZEPgggm1hpj2bTFZ5EDtlzgd2smOaO7Y7YZuIW76RZtrv1dqCXI64qLg5GpVYM2qt3l9OdEQjw9cbY7umZahqOD4dBXBtlSsP9SPc1wCda5bDGfc5yvwkh%2FXibU%2F9i8EVOc6SLbeZ6i4O9mRirk6N3zXCYI%2B5lOJ3Ip%2BJp2tEE7uFmRBiAjl8OPL1RMJ8Lm8b1fZ5JlyYvwdEB%2Bqs%2BMqANXzq3ITbTalUq5t3d67rDRTLJl4a%2FE4LuNwO1K5LU%2B4%2FAACl1yVK1U4WsrMVWzXU%2F7HTgt0ZqdGpvse3D18x14CbagdqEooEAv%2FqFvoV3eo9RlZN6IjlwEkB1ZyMe%2FfQlYkX96cUXwNqYMDhdlzTWC3g9BbVa%2FszJidp3qZ%2FZUmhZvKWOew%2BJwa4XAifSBdQtCu2wPlUt8jVDCLY5T3t9IC3JWPTF3liEq5CEO1LZ4p6Jzt%2BMLUJIfTit2PDY34x%2B4h43MMjGhPkFOusBkcmcIDjGhL%2Fogu5m9iOjliSz5WoGDTupGmkYjL1JWfomqUc%2B2szea7MIHdUB1gD2QyGqmT1F%2BoPX8N%2BgfP4PselYMMUR0yhAdwLTCht4JLky631ps%2BZB6IEIkIRIQcLlXuUjHebJquhtnkQ1D5oZTr8Wt42ELWDiEod6dm0giBdTq8S7CFaVSpryZOmMiXVTa3YNIDcSSjqwNoSSY2du41PoLPCUnueUOhMhpq8TayuAThBjk%2B3nYUc1Ui98I2GqVaA6J3%2Bdre7U7ptw41PM8hsfHzmky8FPtd61ZM4TKeWlQvgKteCP%2FPstHg%3D%3D&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Date=20200729T083723Z&X-Amz-SignedHeaders=host&X-Amz-Expires=300&X-Amz-Credential=ASIAQ3PHCVTYT45SFB7Q%2F20200729%2Fus-east-1%2Fs3%2Faws4_request&X-Amz-Signature=641c72bcbcd0246dfaa09efbe9dc17f11772be5eee21f3c3bd523c5072fe77b4&hash=afcc01f50314618e49093d7661a29fc7a668a0d71371a17b4e3806db9f10a4a4&host=68042c943591013ac2b2430a89b270f6af2c76d8dfd086a07176afe7c76c2c61&pii=S2214785320335057&tid=spdf-0a2b24f7-763f-457f-8a14-3bb13a281d23&sid=6c57fa2c5df636494d5b2d63644c8f0fb75dgxrqb&type=client:application/pdf},
