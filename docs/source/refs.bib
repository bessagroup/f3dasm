@Article{Chin2009,
  author   = {Chin, C. W. and Yousif, B. F.},
  journal  = {Wear},
  title    = {Potential of kenaf fibres as reinforcement for tribological applications},
  year     = {2009},
  issn     = {0043-1648},
  month    = sep,
  number   = {9},
  pages    = {1550--1557},
  volume   = {267},
  abstract = {This paper presents an attempt to use kenaf fibres as reinforcement for tribo-composite based on epoxy for bearing applications. Kenaf fibres reinforced epoxy (KFRE) composite was fabricated using a closed mould technique associated with vacuum system. Sliding wear and frictional behaviour of the composite were studied against polished stainless steel counterface using Block-On-Disc (BOD) machine at different applied loads (30–100N), sliding distances (0–5km) and sliding velocities (1.1–3.9m/s). The effect of the fibre orientations, with respect to the sliding direction, was considered; these orientations are parallel (P-O), anti-parallel (AP-O) and normal (N-O). The morphology of the worn surfaces of the composite was studied using a scanning electron microscope (SEM). The result revealed that the presence of kenaf fibres in the composite enhanced the wear and frictional performance of the epoxy. Applied load and sliding velocity have less effect on the specific wear rate of the composite in all the three orientations. The composite exhibited better wear performance in N-O compared to P-O and AP-O.},
  doi      = {10.1016/j.wear.2009.06.002},
  file     = {ScienceDirect Full Text PDF:https\://pdf.sciencedirectassets.com/271461/1-s2.0-S0043164809X00104/1-s2.0-S0043164809004128/main.pdf?X-Amz-Security-Token=IQoJb3JpZ2luX2VjEEkaCXVzLWVhc3QtMSJGMEQCIAcm3kwUHmZ%2BJYggZC3oL0fYqWq%2BM4cMa16S8M1tTMZ1AiBAD5t9A227ar6seAeaU3%2FspZGQt%2BhAoUrASSt4wXrfNCq0AwgSEAMaDDA1OTAwMzU0Njg2NSIMCeeVvhJuxvVZT52pKpEDSMiPXxVDqA4T6%2BoDLC70i%2FkSGDvTdopJn00fZ3HdwCJ24PSUNjldjPJDSLPJYqPUmCfMYAuc8spYk7AJR4avKbAdY1Y9%2BnqWZC4aA%2BdJ2ITYX1NVbw3lQA9yciDZxPN%2BpauGF%2FM0ncOCiAx5egIOE%2FIci9Y2jE3S5dCAhD%2Fj9Eao8%2BRRBreiSEgudJZEjO9GzEVLavlxEFmZKN8TL%2BHg7WhgSYdvAcSEge6gF1khj%2BoVQgv%2BZLSeWB5QfFiEa80ou5KOBSksISUPBO8w4ZeWguU0UAWiFmBSWLWGj5tk9824OYXFTiMqFlEzCon4z%2Fl7rQimJYFPaYhjHSIJNABQmu9kh59NRHK1LcJ54hwsAvpePgy5TCm32UbL7Q9f1dmLSjJXGr%2BeI2qG9YkU0ZBEjHMh1LtqY7%2F3UcHdy4K%2F9q2fUBm82DHaB%2BzEwXaA%2BjK2gqFS3WlpvZUsygXOIwSK6XIzTMAtDXEHhNUHK0%2BRK0tG3AmeoiqvSBPiMce0WcS%2FK4%2BPmhRRsFKOICTayVpMvgkwgOSE%2BQU67AHnC6sqh3Mf%2BWbAslud98AUIuc9G3eXfE4okoysCH7f5MwbOafeSVA40hc%2Fh%2B%2FuVfAR80j0zjW4XjERC7UvvoRq0PGFRE%2FNxydrh%2BBL5lS4xZTl8oSk2f52yh5I%2BWLYkNeWAgBtbE0vc1O4um%2FaSmhjJRxO7ZAb1%2BonKjcLL0G6CtaOp%2BVViIhLfjbxvhzmyWzP84NRE7bQA0r14GrNsDhtiR4hM6uT%2F7jFsezzhgpNzWWCd7TS6ucz5XN%2FIW%2FAG4DJPJ4JMIODPGYLGna6tfNZ%2BvmxG6xopxFzGnb4OOOkT4ygcxz7dVBByST5%2Bw%3D%3D&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Date=20200729T090412Z&X-Amz-SignedHeaders=host&X-Amz-Expires=300&X-Amz-Credential=ASIAQ3PHCVTY2X4CBCHQ%2F20200729%2Fus-east-1%2Fs3%2Faws4_request&X-Amz-Signature=376be833c68a9037443657d42caaaa7b1aaddce6be366b64a7eb5f3a764f0ff0&hash=a18e85ea2a720c0ab72f8bf740535eb0261479bdb85402046741c63bac69692b&host=68042c943591013ac2b2430a89b270f6af2c76d8dfd086a07176afe7c76c2c61&pii=S0043164809004128&tid=spdf-6dc72f66-72ed-4c54-84a9-a30c04e4c633&sid=6c57fa2c5df636494d5b2d63644c8f0fb75dgxrqb&type=client:application/pdf},
